r.blur(function () {
0 == r.val().length && r.hide().next().show()
}).next().focus(function () {
a(this).hide().prev().show().focus()
})
} else (r.is(":text") || r.is("[type=email]") || r.is("textarea")) && (
r.blur(function () {
